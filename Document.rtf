+using Microsoft.Xna.Framework;\par
+using Microsoft.Xna.Framework.Graphics;\par
+using Microsoft.Xna.Framework.Input;\par
+using System.Collections.Generic;\par
+using Microsoft.Xna.Framework.Input.Touch;\par
+\par
+using System;\par
+\par
+namespace Game1\par
+\{\par
+\par
+    public class Game1 : Game\par
+    \{\par
+        GraphicsDeviceManager graphics;\par
+        SpriteBatch spriteBatch;\par
+        Texture2D texture;\par
+        List<Vector2> snakePos;\par
+        Vector2 foodPos;\par
+        SetSpriteVisitor s_visitor;\par
+        Random r = new Random();\par
+        ButtonFactory buttonfactory = new ButtonFactory();\par
+        List<IButton> ButtonList = new List<IButton>();\par
+        int direction = 1;\par
+        int ldt = -1;\par
+        int LastT = -1;\par
+\par
+        public Game1()\par
+        \{\par
+            graphics = new GraphicsDeviceManager(this);\par
+            Content.RootDirectory = "Content";\par
+            ButtonList.Add(buttonfactory.Create(1));//Create an instance  \par
+            ButtonList.Add(buttonfactory.Create(2));//Create an instance \par
+        \}\par
+\par
+\par
+        protected override void Initialize()\par
+        \{\par
+\par
+            reset();\par
+            base.Initialize();\par
+        \}\par
+\par
+\par
+        protected override void LoadContent()\par
+        \{\par
+            texture = base.Content.Load<Texture2D>("box");\par
+            spriteBatch = new SpriteBatch(GraphicsDevice);\par
+            Texture2D greenbutton = Content.Load<Texture2D>("greenbutton");\par
+            Texture2D redbutton = Content.Load<Texture2D>("redbutton");\par
+            s_visitor = new SetSpriteVisitor(greenbutton, redbutton);\par
+\par
+            foreach (IButton b in ButtonList)\par
+            \{\par
+                b.Visit(s_visitor);\par
+            \}\par
+        \}\par
+\par
+\par
+        protected override void UnloadContent()\par
+        \{\par
+\par
+        \}\par
+\par
+        void reset()\par
+        \{\par
+            direction = 1;\par
+            snakePos = new List<Vector2>();\par
+            for (int i = 0; i < 5; i++)\par
+                snakePos.Add(new Vector2(0, 4 - i));\par
+            foodPos = new Vector2(\par
+                r.Next(0, (GraphicsDevice.PresentationParameters.BackBufferWidth - 1) / 16),\par
+                r.Next(0, (GraphicsDevice.PresentationParameters.BackBufferHeight - 1) / 16));\par
+        \}\par
+\par
+        protected override void Update(GameTime gameTime)\par
+        \{\par
+            TouchCollection touchCollection = TouchPanel.GetState();\par
+            if (touchCollection.Count > 0)\par
+            \{\par
+                if (touchCollection[0].Position.X < 960 && touchCollection[0].Position.Y > 540 && touchCollection[0].Position.Y < 810 && ldt != 1)\par
+                    direction = 0;\par
+                else\par
+                    if (touchCollection[0].Position.X < 960 && touchCollection[0].Position.Y > 810 && ldt != 0)\par
+                    direction = 1;\par
+                else\par
+                        if (touchCollection[0].Position.X > 960 && touchCollection[0].Position.Y > 540 && touchCollection[0].Position.Y < 810 && ldt != 3)\par
+                    direction = 2;\par
+                else\par
+                            if (touchCollection[0].Position.X > 960 && touchCollection[0].Position.Y > 810 && ldt != 2)\par
+                    direction = 3;\par
+            \}\par
+            if (gameTime.TotalGameTime.TotalMilliseconds > LastT + 100)\par
+            \{\par
+\par
+                ldt = direction;\par
+\par
+                for (int i = snakePos.Count - 1; i > 0; i--)\par
+                    snakePos[i] = snakePos[i - 1];\par
+\par
+                if (direction == 0)\par
+                    snakePos[0] += new Vector2(0, -1);\par
+                if (direction == 1)\par
+                    snakePos[0] += new Vector2(0, 1);\par
+                if (direction == 2)\par
+                    snakePos[0] += new Vector2(-1, 0);\par
+                if (direction == 3)\par
+                    snakePos[0] += new Vector2(1, 0);\par
+\par
+                if (snakePos[0].X == foodPos.X && snakePos[0].Y == foodPos.Y)\par
+                \{\par
+                    snakePos.Add(snakePos[snakePos.Count - 1]);\par
+                    foodPos = new Vector2(\par
+                        r.Next(0, (GraphicsDevice.PresentationParameters.BackBufferWidth - 1) / 16),\par
+                        r.Next(0, (GraphicsDevice.PresentationParameters.BackBufferHeight - 1) / 16));\par
+                \}\par
+\par
+                for (int i = 1; i < snakePos.Count; i++)\par
+                    if (snakePos[0].X == snakePos[i].X && snakePos[0].Y == snakePos[i].Y)\par
+                        reset();\par
+\par
+                if (snakePos[0].X * snakePos[0].Y < 0 ||\par
+                    snakePos[0].Y + 1 > GraphicsDevice.PresentationParameters.BackBufferHeight / 16 ||\par
+                    snakePos[0].X + 1 > GraphicsDevice.PresentationParameters.BackBufferWidth / 16)\par
+                    reset();\par
+\par
+                LastT = (int)gameTime.TotalGameTime.TotalMilliseconds;\par
+            \}\par
+            \par
+            if (touchCollection.Count > 0)//just a loop\par
+            \{\par
+                if (touchCollection[0].State == TouchLocationState.Pressed)\par
+                \{\par
+                    if (touchCollection[0].Position.X > 1620 && touchCollection[0].Position.Y < 53)\par
+                        Environment.Exit(0);\par
+                    if (touchCollection[0].Position.X < 244 && touchCollection[0].Position.Y < 50)\par
+                        reset();\par
+                \}\par
+            \}\par
+            base.Update(gameTime);\par
+        \}\par
+\par
+\par
+        protected override void Draw(GameTime gameTime)\par
+        \{\par
+            GraphicsDevice.Clear(Color.White);\par
+\par
+            spriteBatch.Begin();\par
+\par
+\par
+\par
+            foreach (Vector2 pos in snakePos)\par
+                spriteBatch.Draw(texture, new Rectangle((int)pos.X * 16, (int)pos.Y * 16, 15, 15), Color.Blue);\par
+            spriteBatch.Draw(texture, new Rectangle((int)foodPos.X * 16, (int)foodPos.Y * 16, 15, 15), Color.Yellow);\par
+\par
+            foreach (var button in ButtonList)//for every block in the list...\par
+            \{\par
+                button.Draw(spriteBatch);//call the method draw\par
+            \}\par
+            spriteBatch.End();\par
+\par
+\par
+            base.Draw(gameTime);\par
+        \}\par
+    \}\par
+\par
+\} \par
